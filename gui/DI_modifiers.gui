### Modifier Window

window = {
	name = "DI_modifiers_window"
	parentanchor = top|right
	position = { -50 100 }
	size = { 150 100 }
	layer = windows_layer
	filter_mouse = all
	allow_outside = yes
    alwaystransparent = no

    using = Window_Background
	using = Window_Decoration
    movable = yes
	visible = "[GetVariableSystem.Exists('DI_modifiers_window_visible')]"
	
    datacontext = "[GetVariableSystem]"
	
	state = {
		name = _show
		using = Animation_FadeIn_Quick
	}
	
	state = {
		name = _hide
		using = Animation_FadeOut_Quick
	}
	
	
	
	vbox = {
        margin_left = 40
		margin_right = 40
		margin_top = 20
		margin_bottom = 20
		ignoreinvisible = yes
		restrictparent_min = yes
		header_pattern = {
			layoutpolicy_horizontal = expanding
			
			blockoverride "header_text"
			{
				text = DI_modifiers
			}
			
			blockoverride "button_close"
			{
				onclick = "[GetVariableSystem.Toggle('DI_modifiers_window_visible')]"
			}
		}
		
		widget = {
			size = { 450 60 }
            
            vbox = {
				hbox = {
					spacing = 5
					margin = { 10 0 }
					margin_bottom = 5		

					button_round = {
						size = { 28 28 }
						enabled = "[GetScriptedGui('DI_modifier_subtract_increment').IsValid(GuiScope.SetRoot(GetPlayer.MakeScope).End)]"
						onclick = "[GetScriptedGui('DI_modifier_subtract_increment').Execute(GuiScope.SetRoot(GetPlayer.MakeScope).End)]"

						button_minus_small = {
							name = "decrease_skill"
							parentanchor = center
							alwaystransparent = yes
						}
					}

					text_label_center = {
						name = "increment"
						size = { 30 30 }
						
						text = DI_modifier_increment
						tooltip = DI_increment_tt
						default_format = "#low"
					}

					button_round = {
						size = { 28 28 }
						enabled = "[GetScriptedGui('DI_modifier_add_increment').IsValid(GuiScope.SetRoot(GetPlayer.MakeScope).End)]"
						onclick = "[GetScriptedGui('DI_modifier_add_increment').Execute(GuiScope.SetRoot(GetPlayer.MakeScope).End)]"

						button_plus_small = {
							name = "decrease_skill"
							parentanchor = center
							alwaystransparent = yes
						}
					}					
				}
            }
            widget = {
                size = { 450 60 }
            
                widget = {
                
                    size = { 450 60 }
                    position = { 154 -20 }
                    
                    button_checkbox_label = {
                        max_width = 150
                        
                        layoutpolicy_horizontal = expanding
                        onclick = "[GetVariableSystem.Toggle('DI_modifier_mult')]"
                        parentanchor = center
                        
                        
                        blockoverride "checkbox" 
                        {
                            checked = "[GetVariableSystem.Exists('DI_modifier_mult')]"
                        }
                        
                        blockoverride "text"
                        {
                            text = DI_modifier_mult
                        }
                    }
                }
            }

            widget = {
                size = { 450 60 }
            
